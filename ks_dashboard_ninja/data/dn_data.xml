<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
         <record id="config_dn_url" model="ir.config_parameter">
            <field name="key">ks_dashboard_ninja.url</field>
            <field name="value">https://dn16ai.kappso.in</field>
        </record>
    </data>
    
    <record id="ks_my_default_dashboard_board" model="ks_dashboard_ninja.board">
        <field name="name">Default Dashboard</field>
        <field name="ks_dashboard_state">menu</field>
        <field name="ks_dashboard_menu_name">Default Dashboard</field>
        <field name="ks_dashboard_active">true</field>
        <field name="ks_dashboard_default_template" eval="ref('ks_dashboard_ninja.ks_blank')"/>
        <!-- 
        <field name="ks_dashboard_top_menu_id" eval="ref('ks_dashboard_ninja.board_menu_root')"/> 
        -->
    </record>
</odoo>